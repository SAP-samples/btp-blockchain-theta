{
  "welcomeFile": "index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "(trustee/)(.*)",
      "destination": "theta_trustee_be",
      "csrfProtection": true,
      "authenticationType": "none"
    },
    {
      "source": "(offchain/)(.*)",
      "destination": "theta_offchain_be",
      "csrfProtection": true,
      "authenticationType": "none"
    },
    {
      "localDir": "resources",
      "source": "^/(.*)",
      "authenticationType": "none"
    }
  ]
}


